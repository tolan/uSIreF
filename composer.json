{
    "name": "tolan/usiref",
    "description": "PHP micro service framework",
    "version": "0.0.1",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "sort-packages": true,
    "authors": [
        {
            "name": "Martin Kovar",
            "email": "mkovar86@gmail.com"
        }
    ],
    "scripts": {
       "unit": "cd tests/Unit && ../../vendor/bin/phpunit"
    },
    "require": {
        "php": ">=7.0.0",
        "nikic/fast-route": "1.2.0",
        "symfony/process": "4.0.4"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0.0"
    },
    "autoload": {
        "psr-4": {
            "uSIreF\\": "src",
            "uSIref\\Unit\\": "tests\\Unit",
            "uSIref\\Example\\": "examples"
        }
    }
}